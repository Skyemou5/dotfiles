. "$HOME/.cargo/env"

# enable user-wide dark theme via 'brute force'
export GTK_THEME=Adwaita:dark

# clipmenu stuff
# clipmenu clipboard management session-wide
export CM_LAUNCHER=rofi
export CM_MAX_CLIPS=10000

#theme parity for gtk and qt styles
export QT_STYLE_OVERRIDE=adwaita

#screenshots location on hyprland
export HYPRSHOT_DIR="/home/mara/Pictures/Screenshots"
